<?xml version="1.0" encoding="UTF-8"?>
<ROOT>
	<!-- Master -->
	<INTERFACE NAME="IFMB002">
		<TABLE NAME="MST_DEPARTMENT" TABLE="MST_Department"><!-- 部门 -->
			<FIELD NAME="UNIT_ID" TYPE="NUMBER" COLUMN="UNIT_ID" KEY="TRUE" NULLABLE="FALSE"/>
			<FIELD NAME="UNIT_CODE" TYPE="STRING" COLUMN="UNIT_CODE" NULLABLE="TRUE" EXP="TRUE"/>
			<FIELD NAME="DESCRIPTION" TYPE="STRING" COLUMN="DESCRIPTION" NULLABLE="TRUE"/>
			<FIELD NAME="PARENT_UNIT_ID" TYPE="NUMBER" COLUMN="PARENT_UNIT_ID" NULLABLE="TRUE"/>
			<FIELD NAME="ENABLED_FLAG" TYPE="STRING" COLUMN="ENABLED_FLAG" NULLABLE="TRUE"/>
			<FIELD NAME="UNIT_LEVEL" TYPE="NUMBER" COLUMN="UNIT_LEVEL" NULLABLE="TRUE"/>
			<FIELD NAME="FIN_UNIT_CODE" TYPE="STRING" COLUMN="FIN_UNIT_CODE"/>
			<FIELD NAME="DDL_TYPE" TYPE="STRING" COLUMN="DDL_TYPE"/>
			<FIELD NAME="LAST_UPDATE_DATE" TYPE="DATE" COLUMN="LAST_UPDATE_DATE"/>
		</TABLE>
	</INTERFACE>
	<INTERFACE NAME="IFMB003">
		<TABLE NAME="MST_EMPLOYEES" TABLE="EXP_EMPLOYEES_INTERFACE"><!-- 用户 -->
			<FIELD NAME="EMPLOYEE_ID" TYPE="NUMBER" COLUMN="EMPLOYEE_ID" KEY="TRUE" NULLABLE="FALSE"/>
			<FIELD NAME="EFFECTIVE_START_DATE" TYPE="DATE" COLUMN="EFFECTIVE_START_DATE" NULLABLE="TRUE"/>
			<FIELD NAME="EFFECTIVE_END_DATE" TYPE="DATE" COLUMN="EFFECTIVE_END_DATE"/>
			<FIELD NAME="EMPLOYEE_TYPE_ID" TYPE="NUMBER" COLUMN="EMPLOYEE_TYPE_ID" NULLABLE="TRUE"/>
			<FIELD NAME="NAME" TYPE="STRING" COLUMN="NAME" NULLABLE="FALSE"/>
			<FIELD NAME="DATE_OF_BIRTH" TYPE="DATE" COLUMN="DATE_OF_BIRTH"/>
			<FIELD NAME="MARITAL_STATUS" TYPE="STRING" COLUMN="MARITAL_STATUS"/>
			<FIELD NAME="NATIONAL_IDENTIFIER" TYPE="STRING" COLUMN="NATIONAL_IDENTIFIER"/>
			<FIELD NAME="SEX" TYPE="STRING" COLUMN="SEX" NULLABLE="FALSE"/>
			<FIELD NAME="EMPLOYEE_CODE" TYPE="STRING" COLUMN="EMPLOYEE_CODE" NULLABLE="TRUE"/>
			<FIELD NAME="EMAIL" TYPE="STRING" COLUMN="EMAIL" />
			<FIELD NAME="MOBIL" TYPE="STRING" COLUMN="MOBIL"/>
			<FIELD NAME="DDL_TYPE" TYPE="STRING" COLUMN="DDL_TYPE"/>
			<FIELD NAME="LAST_UPDATE_DATE" TYPE="DATE" COLUMN="LAST_UPDATE_DATE"/>
		</TABLE>
	</INTERFACE>
	
		<INTERFACE NAME="IFMB004">
		<TABLE NAME="MST_STATION" TABLE="MST_STATION"><!-- 用户 -->
			<FIELD NAME="POSITION_ID" TYPE="NUMBER" COLUMN="POSITION_ID" KEY="TRUE" NULLABLE="FALSE"/>
			<FIELD NAME="UNIT_ID" TYPE="NUMBER" COLUMN="UNIT_ID" NULLABLE="TRUE"/>
			<FIELD NAME="POSITION_CODE" TYPE="NUMBER" COLUMN="POSITION_CODE"/>
			<FIELD NAME="DESCRIPTION" TYPE="STRING" COLUMN="DESCRIPTION" NULLABLE="TRUE"/>
			<FIELD NAME="EMPLOYEE_JOB_ID" TYPE="STRING" COLUMN="EMPLOYEE_JOB_ID" NULLABLE="TRUE"/>
			<FIELD NAME="ENABLED_FLAG" TYPE="STRING" COLUMN="ENABLED_FLAG"/>
			<FIELD NAME="EFFECTIVE_START_DATE" TYPE="DATE" COLUMN="EFFECTIVE_START_DATE"/>
			<FIELD NAME="EFFECTIVE_END_DATE" TYPE="DATE" COLUMN="EFFECTIVE_END_DATE"/>
			<FIELD NAME="LAST_UPDATE_DATE" TYPE="DATE" COLUMN="LAST_UPDATE_DATE" NULLABLE="TRUE"/>
		</TABLE>
	</INTERFACE>

</ROOT>